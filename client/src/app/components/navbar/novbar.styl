.novbar
    width 100%

.novbar_box
    width 100%
    margin 0 auto
    background #5a5454
    position relative

.novbar_box ul
    border-radius 0
    border none
    font-family Doppio
    text-transform uppercase
    position relative
    display inline-block
    

.novbar_box ul li
    list-style none
    display inline-block
    height 50px
    width 150px
    padding 20px

.novbar_box ul li > a
    text-align center
    text-decoration none
    color #FFF
    border none
    border-radius 3px
    padding 5px


.novbar_box ul li:hover > a
    background grey
    color #FFF
    text-decoration none

.search
    position relative
    float right
    display inline-block
    padding 15px


.edit 
    border 1px solid #9E9E9E
    color #000000
    padding 3px
    margin-top 2px
    margin-bottom 2px
    font-size 11px
    font-family Verdana
    background #FFF
 
.name 
    width 100%
    height 150px
    margin 0 auto
    background gray
    border 1px solid #9E9E9E

.name h1 
    padding 60px 0px 0px 5px
